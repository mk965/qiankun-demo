{
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "start": "npm-run-all --parallel start:*",
    "install:main-react": "cd main-react && npm install",
    "install:sub-react": "cd main-react && npm install",
    "install:sub-vue": "cd main-vue && npm install",
    "install:sub-vue3": "cd main-vue3 && npm install",
    "start:main-react": "cd main-react && npm start",
    "start:sub-react": "cd sub-react && npm start",
    "start:sub-vue": "cd sub-vue && npm start",
    "start:sub-vue3": "cd sub-vue3 && npm start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
